{
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "get",
        "post",
        "put",
        "delete",
        "patch",
        "options"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": ""
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/images/.{png,jpg,gif}",
      "/css/"
    ]
  },
  "platform": {
    "apiRuntime": "node:20"
  }
}